{
  "name": "my-turborepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "prepare": "cd ./apps/web && husky install",
    "build": "turbo run build",
    "dev:u": "cd ./apps/api && docker compose up -d",
    "dev:d": "cd ./apps/api && docker compose down --remove-orphans",
    "dev:web": "cd ./apps/web && pnpm dev",
    "dev:api": "cd ./apps/api && pnpm dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "build:docker:prod": "docker build ./apps/api -t veneno --build-arg APP_ENV=production"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=18.15.0"
  },
  "dependencies": {},
  "packageManager": "pnpm@7.25.1"
}