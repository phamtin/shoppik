{
	"compilerOptions": {
		"strict": true,
		"incremental": true,
		"esModuleInterop": true,
		"target": "es2018",
		"module": "es2020",
		"moduleResolution": "node",
		"lib": ["ES5", "DOM"],
		"types": ["node"],
		"allowSyntheticDefaultImports": true,
		"sourceMap": true,
		"outDir": "dist",
		"skipLibCheck": true,
		"noImplicitAny": true,
		"removeComments": false,
		"forceConsistentCasingInFileNames": true,
		"paths": {
			"prisma/*": ["./prisma/*"],
			"Loaders/*": ["./Loaders/*"],
			"Router/*": ["./Router/*"],
			"Service/*": ["./Service/*"],
			"Repository/*": ["./Repository/*"]
		}
		// "isolatedDeclarations": true
	},
	"exclude": ["node_modules", ".vscode", ".turbo"],
	"tsc-alias": {
		"verbose": false,
		"resolveFullPaths": true
	}
}
